function getExistingRewards(address _investor) internal view returns (uint256) {

    uint256 existingRewards = 0;

    for (uint256 i = 0; i < ownedDeposits[_investor].length; i++) {

        uint256 depositId = ownedDeposits[_investor][i];

        existingRewards += depositState[depositId].claimedAmount;

    }

    return existingRewards;

}
